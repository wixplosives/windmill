{
  "name": "@windmill/scripts",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "prebuild": "yarn clean",
    "build": "yarn build:cjs && yarn build:esm",
    "build:watch": "run-p build:cjs-watch build:esm-watch",
    "build:cjs-watch": "tsc -p tsconfig.build.json --watch --outDir cjs --module commonjs",
    "build:esm-watch": "tsc -p tsconfig.build.json --watch --outDir esm --module esnext",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir cjs",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir esm",
    "test": "mocha \"test/**/*.spec.ts?(x)\"",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "license": "MIT",
  "repository": "git@github.com:wixplosives/windmill.git",
  "sideEffects": false,
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@windmill/utils": "^1.0.0",
    "html-webpack-plugin": "^4.0.4",
    "puppeteer": "^2.1.1",
    "webpack-virtual-modules": "^0.2.1"
  },
  "devDependencies": {
    "@types/puppeteer": "^2.0.1",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-virtual-modules": "^0.1.0"
  }
}
