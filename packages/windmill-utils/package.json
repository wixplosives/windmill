{
  "name": "@wixc3/windmill-utils",
  "version": "1.0.8",
  "main": "cjs/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "prebuild": "yarn clean",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir cjs",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir esm",
    "test": "mocha \"test/**/*.spec.ts?(x)\"",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "html-webpack-plugin": ">=4",
    "puppeteer": ">=1",
    "webpack": ">=4"
  },
  "files": [
    "cjs",
    "esm",
    "src",
    "patch-console.js",
    "static"
  ],
  "license": "MIT",
  "repository": "git@github.com:wixplosives/windmill.git",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@file-services/node": "^2.2.0",
    "@file-services/overlay": "^2.2.0",
    "@file-services/path": "^2.2.0",
    "@file-services/webpack": "^2.2.0",
    "axe-core": "^3.5.5",
    "chalk": "^4.1.0",
    "koa": "^2.13.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.3.0"
  }
}
