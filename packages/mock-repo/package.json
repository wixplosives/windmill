{
    "name": "@windmill/mock-repo",
    "version": "1.0.0",
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs ./esm",
        "prebuild": "yarn clean",
        "build": "yarn build:cjs && yarn build:esm",
        "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir cjs",
        "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir esm",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prepack": "yarn build"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "files": [
        "cjs",
        "esm",
        "src"
    ],
    "license": "MIT",
    "repository": "git@github.com:wixplosives/sample.git",
    "sideEffects": false,
    "private": true,
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@stylable/webpack-plugin": "^3.3.0",
        "@ts-tools/webpack-loader": "^1.1.4",
        "@wixc3/wcs-core": "^1.5.0"
    }
}
